{
  "name": "test-client",
  "version": "0.8.2",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "lint": "tsc",
    "test": "vitest",
    "release": "npm run ci && changeset publish",
    "posttest": "pnpm attw --pack .",
    "fix-zod": "tsx build/remove-zod.ts",
    "size-lite": "banal ./src/index-lite.ts",
    "size-big": "banal ./src/index-big.ts"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@anatine/zod-openapi": "^2.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.9.0",
    "@jest/types": "^29.6.3",
    "@types/jest": "^29.5.4",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "banal": "^1.3.0",
    "esbuild": "^0.18.20",
    "eslint": "^8.48.0",
    "express": "^4.18.2",
    "jest": "^29.6.4",
    "prettier": "^2.8.4",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.3",
    "vitest-tsconfig-paths": "^3.4.1"
  }
}
